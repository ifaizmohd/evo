{
  "extends": ["eslint:recommended"],
  "rules": {
    // Add specific ESLint rules here
    "no-console": "warn",
    "no-unused-vars": "warn",
    "semi": "error",
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "no-trailing-spaces": "error",
    "curly": "error",
    "eqeqeq": "error",
    "arrow-body-style": ["error", "as-needed"],
    "prefer-const": "error",
    "no-var": "error",
    "no-unused-expressions": "warn",
    "no-shadow": "error"
  },
  "env": {
    "es6": true
  }
}
